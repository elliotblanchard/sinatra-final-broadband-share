<H1>Signup</H1>
<form class="" action="/signup" method="post">
    <p>
    <label for="username">Username:</label>
    <input type="text" name="username" id="username" value="">
    <label for="email">Email:</label>
    <input type="text" name="email" id="email" value="">
    </p>
    <p>
    <label for="address">Address:</label>
    <input type="text" name="address" id="address" value="">
    <label for="zip">Zip Code:</label>
    <input type="text" name="zip" id="zip" value="">
    </p>
    <p>
    <label for="password">Password:</label>
    <input type="password" name="password" id="password" value="">
    <label for="password_confirm">Enter Password Again:</label>
    <input type="password" name="password_confirm" id="password_confirm" value="">
    </p>
    <p>
    <label for="user_type">Are you a:</label>
    <input type="radio" name="user_type" value="student">Student</input>
    <input type="radio" name="user_type" value="provider">Provider</input>
    </p>
    <input type="submit" name="" value="Signup">
</form>
